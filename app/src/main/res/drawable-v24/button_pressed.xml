<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <solid
                android:color="#B1B1B1"/>
            <stroke
                android:width="1dp"
                android:color="#5C5C5C"/>
        </shape>
    </item>
</selector>